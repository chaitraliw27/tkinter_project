import pandas as pd

# Sample DataFrame
df = pd.DataFrame({
    'Column1': [1, 2, 3],
    'Column2': [4, 5, 6]
})

# Generate HTML link
html_link = '<a href="diff.html">View Difference</a>'

# Add HTML link as a new column in the DataFrame
df['Difference Link'] = html_link

# Save DataFrame to CSV
df.to_csv('output.csv', index=False)

df.to_csv('output.csv', index=False, escapechar='\\', quoting=pd.QUOTE_NONE)
