import pandas as pd

# Sample DataFrame
df = pd.DataFrame({
    'Column1': [1, 2, 3],
    'Column2': [4, 5, 6]
})

# Generate link
link_text = 'View Difference'
url = 'diff.html'

# Create clickable link in markdown syntax
markdown_link = f'[{link_text}]({url})'

# Add the link as a new column in the DataFrame
df['Difference Link'] = markdown_link

# Save DataFrame to CSV
df.to_csv('output.csv', index=False)
