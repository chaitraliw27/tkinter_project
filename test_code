import pandas as pd
import difflib
from IPython.display import HTML

def html_diff(a, b):
    differ = difflib.HtmlDiff()
    return differ.make_table(a.splitlines(), b.splitlines())

# Sample DataFrames
df1 = pd.DataFrame({'A': [1, 2, 3], 'B': [4, 5, 6]})
df2 = pd.DataFrame({'A': [1, 3, 3], 'B': [4, 6, 6]})

# Convert DataFrames to string representations
df1_str = df1.to_string(index=False)
df2_str = df2.to_string(index=False)

# Generate HTML diff
html_difference = html_diff(df1_str, df2_str)

# Display HTML diff
HTML(html_difference)
