import pandas as pd

# Load CSV files into DataFrames
df1 = pd.read_csv('file1.csv')
df2 = pd.read_csv('file2.csv')

# Iterate over rows
for index, (row1, row2) in enumerate(zip(df1.values, df2.values), start=1):
    # Compare values in corresponding cells
    for col1, col2 in zip(row1, row2):
        if col1 != col2:
            print(f"Difference at row {index}: {col1} != {col2}")
